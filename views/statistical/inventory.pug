extends ../../layouts/navbar-home.pug
block content
  h2 Danh sách hàng tồn kho
    table.table.table-borderless
      thead
        tr
          th Id sản phẩm
          th Tên sản phẩm
          th Số lượng
          th Giá nhập
          th Tổng tiền
      tbody
        each product in products
          tr
            td= product.id
            td= product.name
            td= product.quantily
            td= product.import_price
            td= product.import_price*product.quantily

  
    
  